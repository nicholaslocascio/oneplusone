<thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Pairs Made</th>
    <th>Weeks</th>
    <th></th>
  </tr>
</thead>
   <% if modal == true %>
   <tbody id= "modalPeopleList">
      <% people.each_with_index do |person| %>
        <%= render 'person_row_modal', person: person%>
      <% end %>
    </tbody>
   <% else %>
      <tbody id= "peopleList">
      <% people.each_with_index do |person| %>
        <%= render 'person_row', person: person%>
      <% end %>
    </tbody>
   <% end %>