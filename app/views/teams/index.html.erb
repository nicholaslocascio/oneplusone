<div class="col-md-8 center">

<h2>Teams</h2>
<div class="btn-toolbar">
  <%= link_to new_team_path, class:"btn btn-primary" do %>
  <span class="glyphicon glyphicon-plus"></span> New Team
  <% end %>
</div>
<div class="well">
  <table class="table">
    <thead>
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>Members</th>
      </tr>
    </thead>
    <tbody>

      <%= render @teams%>


    </tbody>
  </table>
</div>

</div>
