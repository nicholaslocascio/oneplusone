<div class="col-md-5 well center well-spaced">
<h1>Create team</h1>




<%= render 'shared/form_errors', errors: @team.errors %>

<%= form_for(@team) do |f| %>

    <div class="field form-group">
            <%= f.label :name%>
      <%= f.text_field :name, class: "form-control", :placeholder => :name %>
    </div>

  <div class="actions">
    <%= f.submit class: "btn btn-lg btn-info btn-block"%>
  </div>
<% end %>



<%= link_to 'Back', teams_path %>

</div>